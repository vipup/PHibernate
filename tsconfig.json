{
  "version": "2.1.0",
  "compileOnSave": false,
  "compilerOptions": {
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "outDir": "lib/",
    "removeComments": false,
    "sourceMap": true,
    "target": "ES6"
  },
  "exclude": [
    "app",
    "lib",
    "node_modules",
    "resources",
    "test",
    "typings/main",
    "typings/main.d.ts",
    "watch",
    "www",
    "www/jspm_packages"
  ]
}